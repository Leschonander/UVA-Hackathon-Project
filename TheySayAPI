from urllib2 import Request, urlopen


def analyse_sent(tip_text):
values = """
  {
    text = + tip_text
  }
"""

headers = {
  'Content-Type': 'application/json; charset=UTF-8'
}
request = Request('https://api.theysay.io/v1/emotion', data=values, headers=headers)

response_body = urlopen(request).read()
print response_body
